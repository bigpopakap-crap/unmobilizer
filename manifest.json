{
  "name": "Unmobilizer",
  "manifest_version": 2,
  "version": "4",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "unmobilize.js"
      ],
      "run_at": "document_end"
    }
  ]
}